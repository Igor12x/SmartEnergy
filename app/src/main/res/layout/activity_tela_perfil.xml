        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="@+id/imageView28"
        app:layout_constraintTop_toTopOf="@+id/imageView28"
        app:srcCompat="@drawable/home_perfil" />

    <Spinner
        android:id="@+id/spinnerCasas"
        android:layout_width="213dp"
        android:layout_height="43dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView28"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toEndOf="@+id/imageView33"
        app:layout_constraintTop_toTopOf="@+id/imageView28" />

    <EditText
        android:id="@+id/plainCEPPerfil"
        android:layout_width="253dp"
        android:layout_height="45dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:fontFamily="@font/mplusrrounded1c_bold"
        android:hint="@string/plainCEPPerfil"
        android:inputType="textPersonName"
        android:textColorHint="@color/borda_edit_text_azul"
        app:layout_constraintEnd_toEndOf="@+id/imageView28"
        app:layout_constraintStart_toStartOf="@+id/imageView28"
        app:layout_constraintTop_toBottomOf="@+id/spinnerCasas" />

    <EditText
        android:id="@+id/plainCidadePerfil"
        android:layout_width="147dp"
        android:layout_height="43dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="5dp"
        android:ems="10"
        android:fontFamily="@font/mplusrrounded1c_bold"
        android:hint="@string/plainCidadePerfil"
        android:inputType="textPersonName"
        android:textColorHint="@color/borda_edit_text_azul"
        app:layout_constraintEnd_toStartOf="@+id/plainEstadoPerfil"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="@+id/imageView28"
        app:layout_constraintTop_toBottomOf="@+id/plainCEPPerfil" />

    <EditText
